#Activate
.venv\Scripts\activate
#run 
python manage.py runserver

#Git Setup (In order)
git init
git branch -M main          # Create main branch
git add .
git commit -m "Initial commit"
git remote add origin https://github.com/Ser3n/BookingSystem
git push -u origin main

#Fix Remote (if needed)
git remote remove origin    # Only if you need to reset remote
git remote add origin https://github.com/Ser3n/BookingSystem

#Fix Push Rejection (choose one option)
# Option 1 - Pull first (if you want to keep remote changes)
git pull origin main --allow-unrelated-histories
git push -u origin main

# Option 2 - Force push (if you want to overwrite remote)
git push -u origin main --force

#Warning: Option 2 will overwrite any remote changes!

#Common Git Commands
git status                    # Check status of changes
git add <file>               # Stage specific file
git commit -m "message"      # Commit with message
git push                     # Push to remote
git pull                     # Pull from remote
git branch                   # List branches
git checkout -b <branch>     # Create and switch to new branch


